        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {

    gs.info('Hello world !!');
    try {
        gs.info("Hello world 1");

        var r = new sn_ws.RESTMessageV2('global.IPInfo', 'GETIPInfo');
        r.setStringParameterNoEscape('ipaddress', current.u_ip_address);
        r.setStringParameterNoEscape('specific_field', 'geo');

        var response = r.execute();
        var responseBody = response.getBody();
        var httpStatus = response.getStatusCode();

        gs.info("HTTP Status: " + httpStatus);
        gs.info("Response Body: " + responseBody);

        var responseObj = JSON.parse(responseBody);

        if (responseObj && responseObj.city) {
            current.u_city = responseObj.city;
            gs.info("City = " + responseObj.city);
            current.update(); // Optional: Ensure this is in an after Business Rule to avoid recursion
        } else {
            gs.info("City not found in response.");
        }

    } catch (ex) {
        gs.info('Uncaught error: ' + ex);
    }

})(current, previous);
]]></script>
